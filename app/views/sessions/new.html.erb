<% provide(:title, 'GoodQuestion - Login')%>

<h1>Login</h1>

<%= form_for(:session, url:sessions_path) do |f| %>
	<p>
		<%= f.label :username, "Username" %>
		<%= f.text_field :username %>
	</p>

	<p>
		<%= f.label :password, "Password" %>
		<%= f.password_field :password %>
	</p>

	<p>
		<%= f.submit "Login" %>
	</p>
<% end %>
